parameters:
	appDir: %rootDir%/app
	logDir: %rootDir%/log
	storageDir: %rootDir%/storage
	tempDir: %rootDir%/temp
	wwwDir: %rootDir%/web

	developer:
		# Run in development state for specific IPs
		ips:
			- 127.0.0.1
			- ::1

		# Additionally you can set secret token instead IP address
		token:


configurator:
	developer: %developer%

	load:
		- %appDir%
